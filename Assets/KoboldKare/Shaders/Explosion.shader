				#endif
				float4 ShadowCoords = float4( 0, 0, 0, 0 );

				#if defined(ASE_NEEDS_FRAG_SHADOWCOORDS)
					#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float4 color47 = IsGammaSpace() ? float4(11.98431,11.98431,11.98431,1) : float4(236.02,236.02,236.02,1);
				Gradient gradient31 = NewGradient( 0, 4, 2, float4( 1, 1, 1, 0 ), float4( 0.9150943, 0.7245994, 0.09927912, 0.2439918 ), float4( 0.8509804, 0.2162986, 0.172549, 0.5258717 ), float4( 0, 0, 0, 1 ), 0, 0, 0, 0, float2( 0, 0 ), float2( 1, 1 ), 0, 0, 0, 0, 0, 0 );
				float3 break33 = IN.ase_texcoord3.xyz;
				float mulTime27 = _TimeParameters.x * 0.25;
				float4 appendResult35 = (float4(break33.x , break33.y , ( break33.z + mulTime27 ) , 0.0));
				float simplePerlin3D8 = snoise( ( appendResult35 + mulTime27 ).xyz*2.0 );
				simplePerlin3D8 = simplePerlin3D8*0.5 + 0.5;
				float temp_output_20_0 = ( 1.0 - pow( abs( ( _ExplosionProgression + 1.0 ) ) , -10.0 ) );
				float4 lerpResult50 = lerp( ( color47 * SampleGradient( gradient31, simplePerlin3D8 ) ) , SampleGradient( gradient31, simplePerlin3D8 ) , temp_output_20_0);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = lerpResult50.rgb;
				float Alpha = SampleGradient( gradient31, simplePerlin3D8 ).a;
				float AlphaClipThreshold = pow( abs( ( ( 1.0 - _ExplosionProgression ) + 1.0 ) ) , -10.0 );
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
					LODDitheringTransition( IN.clipPos.xyz, unity_LODFade.x );
				#endif

				#ifdef ASE_FOG
					Color = MixFog( Color, IN.fogFactor );
				#endif

				return half4( Color, Alpha );
			}

			ENDHLSL
